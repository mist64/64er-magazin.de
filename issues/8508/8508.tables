      <td valign="bottom" class="td5">
        <p class="p5">111101</p>
      </td>
      <td valign="bottom" class="td5">
        <p class="p5">$F4</p>
      </td>
      <td valign="bottom" class="td8">
        <p class="p5">6</p>
      </td>
    </tr>
    <tr>
      <td valign="bottom" class="td5">
        <p class="p5">1100</p>
      </td>
      <td valign="bottom" class="td5">
        <p class="p5">11101</p>
      </td>
      <td valign="bottom" class="td5">
        <p class="p5">$E8</p>
      </td>
      <td valign="bottom" class="td8">
        <p class="p5">5</p>
      </td>
    </tr>
    <tr>
      <td valign="bottom" class="td5">
        <p class="p5">1101</p>
      </td>
      <td valign="bottom" class="td5">
        <p class="p5">111110</p>
      </td>
      <td valign="bottom" class="td5">
        <p class="p5">$F8</p>
      </td>
      <td valign="bottom" class="td8">
        <p class="p5">6</p>
      </td>
    </tr>
    <tr>
      <td valign="bottom" class="td5">
        <p class="p5">1110</p>
      </td>
      <td valign="bottom" class="td5">
        <p class="p5">111111</p>
      </td>
      <td valign="bottom" class="td5">
        <p class="p5">$FC</p>
      </td>
      <td valign="bottom" class="td8">
        <p class="p5">6</p>
      </td>
    </tr>
    <tr>
      <td valign="top" class="td12">
        <p class="p5">1111</p>
      </td>
      <td valign="top" class="td12">
        <p class="p5">100</p>
      </td>
